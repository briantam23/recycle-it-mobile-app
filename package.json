{
  "name": "recycle-capstone",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^8.0.0",
    "axios": "^0.18.0",
    "expo": "^31.0.5",
    "expo-cli": "^2.4.3",
    "firebase": "^5.11.1",
    "grpc": "^1.17.0-pre1",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-elements": "^0.19.1",
    "react-native-firebase": "^5.1.0",
    "react-native-maps": "^0.22.1",
    "react-native-navigation-directions": "^1.0.3",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.18.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/expo": "^30.0.1",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "@types/react": "^15.0.24",
    "@types/react-native": "^0.57.10",
    "@types/react-navigation": "^2.13.2",
    "@types/react-redux": "^6.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux-logger": "^3.0.6",
    "babel-preset-expo": "^5.0.0",
    "jest-expo": "^31.0.0",
    "react-native-typescript-transformer": "^1.2.10",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6"
  }
}
